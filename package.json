{
  "name": "nodejs-typescript-babel",
  "version": "1.0.0",
  "description": "This is sample repository demonstrates how to use NodeJS, TypeScript and Babel",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check --watch",
    "start": "babel-node -x \".ts\" src/index.ts",
    "issues": "babel-node -x \".ts\" src/issues.ts",
    "swagger": "babel-node -x \".ts\" src/swagger.ts",
    "auth": "babel-node -x \".ts\" src/checkSSLCert.ts",
    "wait": "babel-node -x \".ts\" src/waitOnUrl.ts",
    "routes": "babel-node -x \".ts\" src/k8sClientNodeCallExample.ts",
    "yaml": "babel-node -x \".ts\" src/swagger-json-to-yaml.ts",
    "swagger-ui": "babel-node -x \".ts\" src/swagger-ui.ts",
    "git-service": "babel-node -x \".ts\" src/test.ts"
  },
  "engines": {
    "node": "20.x"
  },
  "author": {
    "name": "it-efrem",
    "email": "github@it-efrem.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/node": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash-es": "^4.17.6",
    "@types/make-fetch-happen": "^9.0.1",
    "install": "^0.13.0",
    "json-to-pretty-yaml": "^1.2.2",
    "npm": "^8.12.1",
    "swagger-ui-dist": "^4.12.0",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.20.0",
    "@octokit/core": "^6.1.2",
    "@openshift-console/dynamic-plugin-sdk": "^1.3.0",
    "axios": "^1.6.8",
    "cacheable-request": "^12.0.1",
    "jsonpath": "^1.1.1",
    "lowdash": "^1.2.0",
    "make-fetch-happen": "^13.0.0",
    "openapi-types": "^12.1.3",
    "xterm": "^5.3.0"
  }
}
